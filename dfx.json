{
	"canisters": {
		"console": {
			"candid": "src/console/console.did",
			"package": "console",
			"type": "rust",
			"declarations": {
				"node_compatibility": true
			},
			"optimize": "cycles"
		},
		"observatory": {
			"candid": "src/observatory/observatory.did",
			"package": "observatory",
			"type": "rust",
			"declarations": {
				"node_compatibility": true
			},
			"optimize": "cycles"
		},
		"mission_control": {
			"candid": "src/mission_control/mission_control.did",
			"package": "mission_control",
			"type": "rust",
			"declarations": {
				"node_compatibility": true
			},
			"optimize": "cycles"
		},
		"satellite": {
			"candid": "src/satellite/satellite.did",
			"package": "satellite",
			"type": "rust",
			"declarations": {
				"node_compatibility": true
			},
			"optimize": "cycles",
			"metadata": [{ "name": "supported_certificate_versions", "content": "1,2" }]
		},
		"orbiter": {
			"candid": "src/orbiter/orbiter.did",
			"package": "orbiter",
			"type": "rust",
			"declarations": {
				"node_compatibility": true
			},
			"optimize": "cycles"
		},
		"frontend": {
			"frontend": {
				"entrypoint": "build/index.html"
			},
			"source": ["build/"],
			"type": "assets",
			"declarations": {
				"node_compatibility": true
			}
		}
	},
	"defaults": {
		"build": {
			"args": "",
			"packtool": ""
		},
		"replica": {
			"subnet_type": "system"
		}
	},
	"dfx": "0.18.0",
	"networks": {
		"local": {
			"bind": "127.0.0.1:8000",
			"type": "ephemeral"
		}
	},
	"version": 1
}
